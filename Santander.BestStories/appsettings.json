{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HackerNews": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "Timeout": "00:00:05",
    "BestStoriesCacheTtl": "00:00:30",
    "ItemCacheTtl": "00:10:00",
    "MaxDegreeOfParallelism": 16,
    "PoolMultiplier": 3,
    "PoolMax": 500,
    "MaxN": 200
  },
  "AllowedHosts": "*"
}
